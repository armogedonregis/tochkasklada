{
    "$schema": "https://json.schemastore.org/swcrc",
    "sourceMaps": true,
    "module": {
      "type": "commonjs"
    },
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "decorators": true,
        "dynamicImport": true
      },
      "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true
      },
      "target": "es2021",
      "keepClassNames": true
    },
    "minify": false
  }