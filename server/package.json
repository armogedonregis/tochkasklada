  "main": "server.js",
  "scripts": {
    "dev": "dotenv -e ../.env -- npx ts-node-dev --rs --clear --exit-child src/server.ts",
    "build": "tsc --project tsconfig.json -p tsconfig.json",
    "start": "node dist/server.js",
    "migrate:reset": "dotenv -e ../.env -- npx prisma migrate reset",
