    "migrate:push": "dotenv -e ../.env -- npx prisma db push",
    "migrate:build": "dotenv -e ../.env -- npx prisma migrate deploy",
    "migrate:prod": "prisma generate && prisma migrate deploy",
    "prisma": "npx prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.3.0",
