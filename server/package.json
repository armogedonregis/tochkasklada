    "hyper-express": "^6.17.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "node-fetch": "^3.3.2",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.8",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.13.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
